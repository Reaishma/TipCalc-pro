{
  "version": 2,
  "builds": [
    {
      "src": "client/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "client/index.ts"
    },
    {
      "src": "/(.*)",
      "dest": "index.html"
    }
  ],
  "installCommand": "npm install --prefix client && npm install",
  "buildCommand": "npm run build"
}
